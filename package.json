{
  "name": "webapp-template",
  "description": "A preconfigured template for Parcel with React, TypeScript, TailwindCSS, along with Linting, dependency upgrades and PR checks",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/JTKBowers/webapp-template",
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" \"./*.{js,mjs}\"",
    "check-format": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\" \"./*.{js,mjs}\"",
    "test": "web-test-runner \"src/**/*.test.tsx\""
  },
  "source": "public/index.html",
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.3.2",
    "@parcel/core": "^2.3.2",
    "@parcel/optimizer-css": "^2.3.2",
    "@parcel/transformer-css-experimental": "^2.3.2",
    "@parcel/transformer-postcss": "^2.3.2",
    "@testing-library/react": "^12.1.4",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@web/test-runner": "^0.13.27",
    "@web/test-runner-playwright": "^0.8.8",
    "autoprefixer": "^10.4.2",
    "chai": "^4.3.6",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "parcel": "^2.3.2",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  }
}
