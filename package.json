{
  "name": "webapp-template",
  "description": "A preconfigured template for Parcel with React, TypeScript, TailwindCSS, along with Linting, dependency upgrades and PR checks",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/JTKBowers/webapp-template",
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" \"./*.{js,mjs}\"",
    "check-format": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\" \"./*.{js,mjs}\"",
    "test": "web-test-runner \"src/**/*.test.tsx\""
  },
  "source": "public/index.html",
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@parcel/core": "^2.4.1",
    "@testing-library/react": "^13.1.1",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.0",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@web/test-runner": "^0.13.27",
    "@web/test-runner-playwright": "^0.8.8",
    "autoprefixer": "^10.4.4",
    "chai": "^4.3.6",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "parcel": "^2.4.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "process": "^0.11.10",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  }
}
